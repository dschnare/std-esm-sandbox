{
  "name": "std-esm-sandbox",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node -r @std/esm src/index",
    "test": "nyc mocha -r @std/esm --opts .mocha.opts",
    "js:bundle": "node -r @std/esm ./node_modules/.bin/browserify src/index.js --debug -t uglifyify -p [ tsify ] | exorcist dist/bundle.js.map | uglifyjs -c  > dist/bundle.js",
    "js:bundle:dev": "node -r @std/esm ./node_modules/.bin/watchify src/index.js -p [ tsify ] --outfile dist/bundle.js"
  },
  "@std/esm": {
    "esm": "all",
    "cjs": true
  },
  "keywords": [],
  "author": "Darren Schnare <schnare.d@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@std/esm": "0.2.1"
  },
  "devDependencies": {
    "@types/mocha": "2.2.41",
    "@types/node": "8.0.20",
    "browserify": "14.4.0",
    "exorcist": "0.4.0",
    "mocha": "3.5.0",
    "nyc": "11.1.0",
    "tsify": "3.0.1",
    "typescript": "2.4.2",
    "uglify-js": "3.0.27",
    "uglifyify": "4.0.3",
    "watchify": "3.9.0"
  }
}
