    "@types/mocha": "2.2.41",
    "@types/node": "8.0.20",
    "browserify": "14.4.0",
    "chalk": "2.1.0",
    "copy": "0.3.0",
    "exorcist": "0.4.0",
    "glob": "7.1.2",
    "mkdirp": "0.5.1",
    "mocha": "3.5.0",
    "nyc": "11.1.0",
    "rimraf": "2.6.1",
    "tsify": "3.0.1",
    "typescript": "2.4.2",
    "uglify-js": "3.0.27",
